+        case 'S':
+            switch (buf[tmpIndex+1]) {
+                case 'a':
+                    returnValue = SAT;
+                    *index += bufferIndexIncrementGivenNTest(buf, tmpIndex+3, 'u', sizeof("Saturday"), 3);
+                    break;
+                case 'u':
+                    returnValue = SUN;
+                    *index += bufferIndexIncrementGivenNTest(buf, tmpIndex+3, 'd', sizeof("Sunday"), 3);
+                    break;
+            }
+            break;
