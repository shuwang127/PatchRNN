+ *  Dump the actual strings
+ */
+    (*writefn)(handle, T("\nStrings\n"));
+    for (blkp = &bStatsBlks[bStatsBlksMax - 1]; blkp >= bStatsBlks; blkp--) {
+        if (blkp->ptr) {
+            cp = (char_t*) ((char*) blkp->ptr + sizeof(bType));
+            fp = blkp->who;
+            if (gisalnum(*cp)) {
+                (*writefn)(handle, T("%-50s allocated by %s\n"), cp, 
+                    fp->file);
+            }
+        }
+    }
+    free(files);
+    recurseProtect--;
