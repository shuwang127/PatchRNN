+    a_assert(websValid(wp));
+    a_assert(origin == SEEK_SET || origin == SEEK_CUR || origin == SEEK_END);
+    a_assert(wp->docfd >= 0);
+
+    wip = &websRomPageIndex[wp->docfd];
+
+    if (origin != SEEK_SET && origin != SEEK_CUR && origin != SEEK_END) {
+        errno = EINVAL;
+        return -1;
+    }
+    if (wp->docfd < 0) {
+        errno = EBADF;
+        return -1;
+    }
+    pos = offset;
+    switch (origin) {
+    case SEEK_CUR:
+        pos = wip->pos + offset;
+        break;
+    case SEEK_END:
+        pos = wip->size + offset;
+        break;
+    default:
+        break;
+    }
+    if (pos < 0) {
+        errno = EBADF;
+        return -1;
+    }
+    return (wip->pos = pos);
 }
 
 #endif /* WEBS_PAGE_ROM */
