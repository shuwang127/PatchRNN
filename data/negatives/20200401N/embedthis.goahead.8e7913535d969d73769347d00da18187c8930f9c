-		ip->putBackToken = bstrdup(B_L, state->putBackToken);
-	}
+    ejinput_t   *ip;
+
+    a_assert(ep);
+
+    ip = ep->input;
+    a_assert(ip);
+
+    ip->tokbuf = state->tokbuf;
+    ip->script = state->script;
+    ip->putBackTokenId = state->putBackTokenId;
+    if (ip->putBackToken) {
+        bfree(B_L, ip->putBackToken);
+    }
+    if (state->putBackToken) {
+        ip->putBackToken = bstrdup(B_L, state->putBackToken);
+    }
 }
 
 
