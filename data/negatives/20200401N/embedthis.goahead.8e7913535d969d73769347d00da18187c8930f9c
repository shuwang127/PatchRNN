+            break;
+        case EXPR_GREATER:
+            lval = gstrcmp(lhs, rhs) > 0;
+            break;
+        case EXPR_GREATEREQ:
+            lval = gstrcmp(lhs, rhs) >= 0;
+            break;
+        case EXPR_EQ:
+            lval = gstrcmp(lhs, rhs) == 0;
+            break;
+        case EXPR_NOTEQ:
+            lval = gstrcmp(lhs, rhs) != 0;
+            break;
+        case EXPR_INC:
+        case EXPR_DEC:
+        case EXPR_MINUS:
+        case EXPR_DIV:
+        case EXPR_MOD:
+        case EXPR_LSHIFT:
+        case EXPR_RSHIFT:
+        default:
+            ejError(ep, T("Bad operator"));
+            return -1;
+        }
+    }
+
+    stritoa(lval, buf, sizeof(buf));
+    setString(B_L, &ep->result, buf);
+    return 0;
 }
 
 
