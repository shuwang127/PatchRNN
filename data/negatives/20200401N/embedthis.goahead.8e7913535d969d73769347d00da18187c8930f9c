+    if (ringqPutBlkMax(rq) < (int) sizeof(char_t) && !ringqGrow(rq)) {
+        return -1;
+    }
+    if (rq->servp <= rq->buf) {
+        rq->servp = rq->endbuf;
+    }
+    cp = (char_t*) rq->servp;
+    *--cp = (char_t) c;
+    rq->servp = (unsigned char *) cp;
+    return 0;
 }
 
 
