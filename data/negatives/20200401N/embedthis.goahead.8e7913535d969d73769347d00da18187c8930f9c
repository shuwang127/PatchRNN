+    bStats[q].inuse++;
+    bStatsMemInUse += size;
+    if (bStatsMemInUse > bStatsMemMax) {
+        bStatsMemMax = bStatsMemInUse;
+    }
+    memSize = (1 << (B_SHIFT + q)) + sizeof(bType);
