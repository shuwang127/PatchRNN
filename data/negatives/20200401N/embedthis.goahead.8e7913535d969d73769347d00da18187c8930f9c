+    while ((c = *fmt++) != '\0') {
+        if (c != '%' || (c = *fmt++) == '%') {
+            put_char(&buf, c);
+        } else {
+            enum flag f = flag_none;
+            int width = 0;
+            int prec = -1;
+            for ( ; c != '\0'; c = *fmt++) {
+                if (c == '-') { 
+                    f |= flag_minus; 
+                } else if (c == '+') { 
+                    f |= flag_plus; 
+                } else if (c == ' ') { 
+                    f |= flag_space; 
+                } else if (c == '#') { 
+                    f |= flag_hash; 
+                } else if (c == '0') { 
+                    f |= flag_zero; 
+                } else {
+                    break;
+                }
+            }
+            if (c == '*') {
+                width = va_arg(arg, int);
+                if (width < 0) {
+                    f |= flag_minus;
+                    width = -width;
+                }
+                c = *fmt++;
+            } else {
+                for ( ; gisdigit((int)c); c = *fmt++) {
+                    width = width * 10 + (c - '0');
+                }
+            }
