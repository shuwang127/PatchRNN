+            p += sizeof(bp->u);
+            while (p < (bFreeBuf + bFreeSize) && *p == B_FILL_CHAR) {
+                p++;
+            }
+        } else {
+            a_assert(((bp->flags & ~B_MALLOCED) == B_INTEGRITY) ||
+                bp->flags == B_FILL_WORD);
+            p += (sizeof(bType) + bp->u.size);
+            while (p < (bFreeBuf + bFreeSize) && *p == B_FILL_CHAR) {
